SELECT * FROM your_table_name WHERE :input = Item_Shade OR (:input != Item_Shade AND :input = Shade_Short_Name) OR (:input != Item_Shade AND :input != Shade_Short_Name AND :input = Item_Shade_Description) OR (:input != Item_Shade AND :input != Shade_Short_Name AND :input != Item_Shade_Description AND :input LIKE '%' || Shade_Short_Name || '%') OR (:input != Item_Shade AND :input != Shade_Short_Name AND :input != Item_Shade_Description AND :input NOT LIKE '%' || Shade_Short_Name || '%' AND :input LIKE '%' || Item_Shade_Description || '%');
